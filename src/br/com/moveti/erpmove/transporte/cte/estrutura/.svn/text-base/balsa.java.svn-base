package br.com.moveti.erpmove.transporte.cte.xml;

import com.thoughtworks.xstream.annotations.XStreamAlias;

/**
 * Grupo de informações das balsas - Não Obrigatório
 * @author Jéssica
 */
@XStreamAlias("balsa")
public class balsa {
    @XStreamAlias("xBalsa")
    private String xBalsa;
    @XStreamAlias("nViag")
    private String nViag;
    @XStreamAlias("direc")
    private String direc;
    @XStreamAlias("prtEmb")
    private String prtEmb;  
    @XStreamAlias("prtTrans")
    private String prtTrans;
    @XStreamAlias("prtDest")
    private String prtDest;
    @XStreamAlias("tpNav")
    private String tpNav;
    @XStreamAlias("irin")
    private String irin;

    /**
     * @return the xBalsa
     */
    public String getxBalsa() {
        return xBalsa;
    }

    /**
     * @param xBalsa the xBalsa to set
     */
    public void setxBalsa(String xBalsa) {
        this.xBalsa = xBalsa;
    }

    /**
     * @return the nViag
     */
    public String getnViag() {
        return nViag;
    }

    /**
     * @param nViag the nViag to set
     */
    public void setnViag(String nViag) {
        this.nViag = nViag;
    }

    /**
     * @return the direc
     */
    public String getDirec() {
        return direc;
    }

    /**
     * @param direc the direc to set
     */
    public void setDirec(String direc) {
        this.direc = direc;
    }

    /**
     * @return the prtEmb
     */
    public String getPrtEmb() {
        return prtEmb;
    }

    /**
     * @param prtEmb the prtEmb to set
     */
    public void setPrtEmb(String prtEmb) {
        this.prtEmb = prtEmb;
    }

    /**
     * @return the prtTrans
     */
    public String getPrtTrans() {
        return prtTrans;
    }

    /**
     * @param prtTrans the prtTrans to set
     */
    public void setPrtTrans(String prtTrans) {
        this.prtTrans = prtTrans;
    }

    /**
     * @return the prtDest
     */
    public String getPrtDest() {
        return prtDest;
    }

    /**
     * @param prtDest the prtDest to set
     */
    public void setPrtDest(String prtDest) {
        this.prtDest = prtDest;
    }

    /**
     * @return the tpNav
     */
    public String getTpNav() {
        return tpNav;
    }

    /**
     * @param tpNav the tpNav to set
     */
    public void setTpNav(String tpNav) {
        this.tpNav = tpNav;
    }

    /**
     * @return the irin
     */
    public String getIrin() {
        return irin;
    }

    /**
     * @param irin the irin to set
     */
    public void setIrin(String irin) {
        this.irin = irin;
    }
}
