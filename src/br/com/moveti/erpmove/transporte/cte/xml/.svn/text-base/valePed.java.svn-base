package br.com.moveti.erpmove.transporte.cte.xml;

import com.thoughtworks.xstream.annotations.XStreamAlias;

/**
 * Informações de Vale Pedágio - Não Obrigatório [0-n]
 * @author Jéssica
 */
@XStreamAlias("valePed")
public class valePed {
    @XStreamAlias("CNPJForn")
    private String CNPJForn;
    @XStreamAlias("nCompra")
    private String nCompra;
    @XStreamAlias("CNPJPg")
    private String CNPJPg;

    /**
     * @return the CNPJForn
     */
    public String getCNPJForn() {
        return CNPJForn;
    }

    /**
     * @param CNPJForn the CNPJForn to set
     */
    public void setCNPJForn(String CNPJForn) {
        this.CNPJForn = CNPJForn;
    }

    /**
     * @return the nCompra
     */
    public String getnCompra() {
        return nCompra;
    }

    /**
     * @param nCompra the nCompra to set
     */
    public void setnCompra(String nCompra) {
        this.nCompra = nCompra;
    }

    /**
     * @return the CNPJPg
     */
    public String getCNPJPg() {
        return CNPJPg;
    }

    /**
     * @param CNPJPg the CNPJPg to set
     */
    public void setCNPJPg(String CNPJPg) {
        this.CNPJPg = CNPJPg;
    }
}
