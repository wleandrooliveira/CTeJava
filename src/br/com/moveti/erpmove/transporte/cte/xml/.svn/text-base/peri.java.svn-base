package br.com.moveti.erpmove.transporte.cte.xml;

import com.thoughtworks.xstream.annotations.XStreamAlias;

/**
 * Preenchido quando for transporte de produtos classificados pela ONU como perigosos. - Não Obrigatório
 * 
 * @author Jéssica
 */
@XStreamAlias("peri")
public class peri {
    @XStreamAlias("nONU")
    private String nONU;
    @XStreamAlias("xNomeAE")
    private String xNomeAE;
    @XStreamAlias("xClaRisco")
    private String xClaRisco;
    @XStreamAlias("grEmb")
    private String grEmb;
    @XStreamAlias("qTotProd")
    private String qTotProd;
    @XStreamAlias("qVolTipo")
    private String qVolTipo;
    @XStreamAlias("pontoFulgor")
    private String pontoFulgor;

    /**
     * @return the nONU
     */
    public String getnONU() {
        return nONU;
    }

    /**
     * @param nONU the nONU to set
     */
    public void setnONU(String nONU) {
        this.nONU = nONU;
    }

    /**
     * @return the xNomeAE
     */
    public String getxNomeAE() {
        return xNomeAE;
    }

    /**
     * @param xNomeAE the xNomeAE to set
     */
    public void setxNomeAE(String xNomeAE) {
        this.xNomeAE = xNomeAE;
    }

    /**
     * @return the xClaRisco
     */
    public String getxClaRisco() {
        return xClaRisco;
    }

    /**
     * @param xClaRisco the xClaRisco to set
     */
    public void setxClaRisco(String xClaRisco) {
        this.xClaRisco = xClaRisco;
    }

    /**
     * @return the grEmb
     */
    public String getGrEmb() {
        return grEmb;
    }

    /**
     * @param grEmb the grEmb to set
     */
    public void setGrEmb(String grEmb) {
        this.grEmb = grEmb;
    }

    /**
     * @return the qTotProd
     */
    public String getqTotProd() {
        return qTotProd;
    }

    /**
     * @param qTotProd the qTotProd to set
     */
    public void setqTotProd(String qTotProd) {
        this.qTotProd = qTotProd;
    }

    /**
     * @return the qVolTipo
     */
    public String getqVolTipo() {
        return qVolTipo;
    }

    /**
     * @param qVolTipo the qVolTipo to set
     */
    public void setqVolTipo(String qVolTipo) {
        this.qVolTipo = qVolTipo;
    }

    /**
     * @return the pontoFulgor
     */
    public String getPontoFulgor() {
        return pontoFulgor;
    }

    /**
     * @param pontoFulgor the pontoFulgor to set
     */
    public void setPontoFulgor(String pontoFulgor) {
        this.pontoFulgor = pontoFulgor;
    }
    
}
