package br.com.moveti.erpmove.transporte.cte.xml;

import com.thoughtworks.xstream.annotations.XStreamAlias;

/**
 * Informações do Rodoviário - Obrigatório
 * @author Jéssica
 */
@XStreamAlias("rodo")
public class rodo {
    @XStreamAlias("RNTRC")    
    private String RNTRC;
    @XStreamAlias("dPrev")
    private String dPrev;
    @XStreamAlias("lota")
    private String lota;
    @XStreamAlias("CIOT")
    private String CIOT;
    private occ occ = new occ();
    private valePed valePed = new valePed();    
    private veic veic = new veic();
    private lacRodo lacRodo = new lacRodo();
    private moto moto = new moto();

    /**
     * @return the RNTRC
     */
    public String getRNTRC() {
        return RNTRC;
    }

    /**
     * @param RNTRC the RNTRC to set
     */
    public void setRNTRC(String RNTRC) {
        this.RNTRC = RNTRC;
    }

    /**
     * @return the dPrev
     */
    public String getdPrev() {
        return dPrev;
    }

    /**
     * @param dPrev the dPrev to set
     */
    public void setdPrev(String dPrev) {
        this.dPrev = dPrev;
    }

    /**
     * @return the lota
     */
    public String getLota() {
        return lota;
    }

    /**
     * @param lota the lota to set
     */
    public void setLota(String lota) {
        this.lota = lota;
    }

    /**
     * @return the CIOT
     */
    public String getCIOT() {
        return CIOT;
    }

    /**
     * @param CIOT the CIOT to set
     */
    public void setCIOT(String CIOT) {
        this.CIOT = CIOT;
    }

    public occ getOcc() {
        return occ;
    }

    public void setOcc(occ occ) {
        this.occ = occ;
    }

    public valePed getValePed() {
        return valePed;
    }

    public void setValePed(valePed valePed) {
        this.valePed = valePed;
    }

    public veic getVeic() {
        return veic;
    }

    public void setVeic(veic veic) {
        this.veic = veic;
    }

    public lacRodo getLacRodo() {
        return lacRodo;
    }

    public void setLacRodo(lacRodo lacRodo) {
        this.lacRodo = lacRodo;
    }

    public moto getMoto() {
        return moto;
    }

    public void setMoto(moto moto) {
        this.moto = moto;
    }
}
